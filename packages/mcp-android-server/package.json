{
  "name": "@gbox.ai/mcp-android-server",
  "version": "0.0.4",
  "description": "MCP server exposing Gbox Android control tools via Model Context Protocol",
  "homepage": "https://gbox.ai",
  "author": "Gbox Team",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "@gbox.ai/mcp-android-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector tsx src/index.ts",
    "dev": "node --loader ts-node/esm --no-warnings src/index.ts"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "gbox",
    "ai",
    "android",
    "control"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babelcloud/gbox.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "gbox-sdk": "^0.26.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
